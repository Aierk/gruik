<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link rel="stylesheet" href="{{ asset('semantic/dist/semantic.min.css') }}">
        <link rel="stylesheet" href="{{ asset('css/main.css') }}">
        <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,300,700,800,800italic,700italic,600,600italic,400italic,300italic' rel='stylesheet' type='text/css'>

        {% block head %}{% endblock %}
    </head>
    <body class="{% block bodyclass %}{% endblock %}">
        {% include '@App/default/navbar.html.twig' %}
        {% block content %}{% endblock %}

        <script src="{{ asset('vendor/dist/jquery.js') }}"></script>
        <script src="{{ asset('semantic/dist/semantic.min.js') }}"></script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
